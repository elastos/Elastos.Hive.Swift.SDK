  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
   pod 'Swifter', '~> 1.4.6'
   pod 'Unirest', '~> 1.1.4'
   pod 'Alamofire'
   pod 'PromiseKit'
  
  target 'ElastosHiveSDKTests' do
    inherit! :search_paths
    pod 'Swifter', '~> 1.4.6'
    pod 'Unirest', '~> 1.1.4'
    pod 'PromiseKit'
    pod 'Alamofire'

  end

  target 'TestHost' do
    inherit! :search_paths
    pod 'Swifter', '~> 1.4.6'
    pod 'Unirest', '~> 1.1.4'
    pod 'PromiseKit'
    pod 'Alamofire'

  end


end
